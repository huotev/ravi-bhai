function isIOSApp() {
        var userAgent = window.navigator.userAgent.toLowerCase(),
    safari = /safari/.test( userAgent ),
    ios = /iphone|ipod|ipad/.test( userAgent );

    if( ios ) {
        if ( safari ) {
            return true;
        } else if ( !safari ) {
            return true;
        };
    }
    if(navigator.userAgent.toLowerCase().match(/\(ip.*applewebkit(?!.*(version|crios)).*mobile(\/[a-z0-9]+)?$/)){
        return true;
    }
    return false;
    }
    function app3_sendToApp(payload) {    const encodedData = window.btoa(JSON.stringify(payload))
        if (window.android && window.android.__externalCall) window.android.__externalCall(encodedData)
        if (window.__externalCall) window.__externalCall(encodedData)
        if (app_isIOS() && window.webkit.messageHandlers && window.webkit.messageHandlers.callback) window.webkit.messageHandlers.callback.postMessage(encodedData)
    }
    var customerID = localStorage.getItem('userid');
    var customerToken = localStorage.getItem('authtoken');
    var body = document.getElementsByTagName('body')[0];
    var jsFile = document.createElement("script");
    let isValidToken = false;
    jsFile .type = "text/javascript";

    // jsFile.src = "https://toolassets.haptikapi.com/platform/test-build/build/loader.js";
    jsFile.src = "https://toolassets.haptikapi.com/platform/javascript-xdk/production/loader.js";
    
    jsFile.charset = "UTF-8";

    var refer_lite = localStorage.getItem('nms_refer_lite');
    var app_os = localStorage.getItem('jio_app_os');
    var is_ios = false;
if(app_os != null && app_os != "" ){
        var os = app_os.toLowerCase();
        if(os == 'ios'){
            is_ios = true;
        }
    } else {
        if(isIOSApp()){
            is_ios = true;
        }
    }
    if(localStorage.getItem("authtokenexpire")){
		let today = new Date();
		let expiry = localStorage.getItem("authtokenexpire");
		let expiry_date = new Date(expiry.replace(/-/g, "/"));
		if(today.getTime() <= expiry_date.getTime()){
            isValidToken = true;
		}
	}
    if(customerID != null && customerID != "0" && isValidToken == true && customerToken != null && customerToken != ""){
        window.haptikInitSettings = {
            "business-id": 1277,
            "client-id": "505d15526856ea83ee70f96af2f4ffa21757f8d8",
            "base-url": "https://jio-api.haptikapi.com/",
            botEvents: (event, callback) => {
                if (event && event.event_name === "external_link") {
                    if(event.hasOwnProperty("data"),event.data.hasOwnProperty("link"), (event.data.link != null && event.data.link != "")){
                        if(localStorage.getItem("nms_refer_lite") != undefined && localStorage.getItem("nms_refer_lite") != null && localStorage.getItem("nms_refer_lite") != ""){
                            if(localStorage.getItem("nms_refer_lite") == "jiomartapp"){
                                app_open_external_link(event.data.link);
                            }
                            if(localStorage.getItem("nms_refer_lite") == "jio"){
                                open_external_link(event.data.link);
                            }
                        } else {
                            window.open(event.data.link, '_blank');
                        }
                    } 
                } 
                if (event && event.event_name === "upload_file") {
                    const elem = document.querySelector("#upload-file");
                    elem.dispatchEvent(new MouseEvent("click"));
                    if(app_os == null || app_os == undefined || app_os == "" || is_ios == true){
                        elem.onchange = function(){
                            if(elem.files.length > 0){
                                for(var i = 0; i < elem.files.length; i++){
                                    callback(elem.files[i]);
                                }
                            }
                        };
                    } else {
                        app3_sendToApp({ "type": "gallery" });
                        window.sendCapturedImageFromGallery = (base64encodeImage) => {
                            if (base64encodeImage) {
                                var url = 'data:image/jpeg;base64,' + base64encodeImage;
                                var filename = Math.floor(Date.now() / 1000);
                                if(url){
                                    return fetch(url)
                                    .then((res) => res.blob())
                                    .then((blob) => {
                                        const file = new File([blob], filename, {
                                            type: "image/jpeg",
                                        });
                                        callback(file);
                                    });
                                }
                            }
                        };     
                    }
                }
            },
            xdk: true,
        };
        body.appendChild(jsFile);
    }
